# WABB.in WhatsApp Integration Configuration
# Add these environment variables to your .env file

# WABB API Configuration
WABB_API_URL=https://api.wabb.in
WABB_API_KEY=your_wabb_api_key_here
WABB_WEBHOOK_URL=https://api.wabb.in/webhooks/your_webhook_id

# WABB Webhook Security Token
WABB_WEBHOOK_TOKEN=your_secure_webhook_token_here

# WhatsApp Business API Configuration (if using Meta's API directly)
WHATSAPP_WEBHOOK_VERIFY_TOKEN=your_whatsapp_verify_token_here

# Example webhook URLs for your application
# Replace 'your-domain.com' with your actual domain
# Development: http://localhost:5001/api/webhook/wabb-webhook
# Production: https://your-domain.com/api/webhook/wabb-webhook
